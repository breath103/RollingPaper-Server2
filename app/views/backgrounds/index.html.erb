<h1>Backgrounds</h1>
	<% @backgrounds.each { |e| %>
		<div>
			<img src='<%=e.image.url("original")%>'/>
		</div>
	<% } %>
	
	<div>
		<h2>create new</h2>
		<%= form_for @background, :url => {action: "create"}, :html => { :multipart => true } do |f| %>
		  <%= f.file_field :image %>
		  <%= f.submit "Create" %>
		<% end %>
	</div>
	
	